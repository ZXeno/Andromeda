<Window x:Class="Andromeda.CLI_Prompt" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Command Line Prompt" Height="100" Width="600" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <TextBox x:Name="CommandBox" Margin="10,10,10,0" TextWrapping="NoWrap" Text="TextBox" TextChanged="CommandBox_TextChanged" Height="22" VerticalAlignment="Top" />
        <Button x:Name="Cancel_Button" Content="Cancel" Margin="507,0,10,10" Click="Cancel_Button_Click" Height="16" VerticalAlignment="Bottom"/>
        <Button x:Name="Okay_Button" Content="Okay" HorizontalAlignment="Right" Margin="0,0,90,10" Width="75" Click="Okay_Button_Click" Height="16" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
